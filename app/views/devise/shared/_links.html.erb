<% end %>
<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Olvidaches o teu contrasinal?", new_password_path(resource_name), class: "text-blue-500 hover:text-blue-700" %><br />
<% end %>
